{
  "name": "teatime-components",
  "version": "0.3.13",
  "description": "React components + CSS Modules",
  "scripts": {
    "cleanup": "git reset --hard",
    "compile": "babel component --out-dir component && babel mixin --out-dir mixin && babel tools --out-dir tools && babel view --out-dir view",
    "demo": "webpack --config demo/webpack.config.js",
    "lint": "eslint component/*.js demo/*.js mixin/*.js test/**/*.js view/*.js *.js",
    "prepublish": "in-publish && npm run -s compile || in-install",
    "publish": "in-publish && npm run -s cleanup || in-install",
    "start": "webpack-dev-server --config demo/webpack.config.js --no-info",
    "test": "tape --require ./test/setup test/**/*.js"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "components",
    "css-modules",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sullenor/teatime-components.git"
  },
  "author": "Alexey Litvinov",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "react-outside-event": "^1.2.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.1",
    "eslint": "^2.10.2",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.0.3",
    "html-webpack-plugin": "^2.17.0",
    "in-publish": "^2.0.0",
    "json-loader": "^0.5.4",
    "npm-install-webpack-plugin": "^3.1.3",
    "postcss-loader": "^0.9.1",
    "postcss-url": "^5.1.2",
    "pre-commit": "^1.1.3",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "skin-deep": "^0.16.0",
    "style-loader": "^0.13.1",
    "tape": "^4.5.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  }
}
